package dataStructure.tree;

import java.util.LinkedList;
import java.util.Queue;

public class NormalTree {
    Node root;

    public void addData(int key) {
        Node newNode = new Node();
        newNode.key = key;
        if (null == root) {
            root = newNode;
        } else {
            Queue<Node> queue = new LinkedList<>();
            queue.offer(root);  	
        }
    }
}
